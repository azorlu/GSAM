@model Tournament

<div class="well">
    <h3>
        <strong>@Model.Name</strong>
        <span class="pull-right label label-primary">
            @Model.Category
        </span>
    </h3>
    @foreach (var te in Model.TournamentEvents)
    {
        @Html.Partial("TournamentEventSummary", te)
    }

</div>
